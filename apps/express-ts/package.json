    "@x402/express": "2.0.0",
    "@x402/core": "2.0.0",
    "@x402/evm": "2.0.0",
    "express": "4.18.2",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "uuid": "11.1.0",
    "viem": "2.21.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17",
    "@types/node": "20.10.0",
    "@types/uuid": "9.0.7",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
